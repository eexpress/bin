[alias]
	ci = commit
	st = status
	co = checkout
	df = diff
	df1= diff HEAD^
	df2= diff HEAD^^
	df3= diff master~3
	df4= diff master~4
	d1d	= diff master@{'yesterday'}
	d2d	= diff master@{'2 days ago'}
	d3d	= diff master@{'3 days ago'}
	dflog = log -p
	ls = ls-files
	lg = log --graph -20 --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %Cgreen(%cr)%Creset %s'
	ll = reflog
	web = instaweb

[color]
	diff = always
#	ui = true
#git config --global core.pager 'less -r'
[core]
	pager = cat
#	pager = less -r
[user]
	name = eexpress
	email = eexpress@163.com
[push]
	default = simple
[core]
	quotepath = false
[diff]
	tool = meld
