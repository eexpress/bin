[alias]
	ci = commit
	st = status
	co = checkout
	df = diff
	df1= diff HEAD^
	df2= diff HEAD^^
	df3= diff master~3
	df4= diff master~4
	d1d	= diff master@{'yesterday'}
	d2d	= diff master@{'2 days ago'}
	d3d	= diff master@{'3 days ago'}
	dflog = log -p
	ls = !sh -c 'git ls-files|ascii2uni -a K'
	lg = log --graph -20 --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %Cgreen(%cr)%Creset %s'
	ll = reflog
	web = instaweb

[color]
	diff = always
[user]
	name = eexpress
	email = eexpress@163.com
[http]
	sslVerify = false
	proxy=http://localhost:8087
[push]
	default = simple
